# Hugging Face API (обязательно)
# токен на https://huggingface.co/settings/tokens
HUGGINGFACE_API_TOKEN=your_hf_token_here

# Модель LLM (опционально, по умолчанию Qwen/Qwen2.5-VL-72B-Instruct:ovhcloud)
HF_LLM_MODEL=Qwen/Qwen2.5-VL-72B-Instruct:ovhcloud

# Langfuse (опционально, для трейсинга)
# ключи на https://cloud.langfuse.com
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key
LANGFUSE_SECRET_KEY=your_langfuse_secret_key
LANGFUSE_HOST=https://cloud.langfuse.com

# FastAPI (опционально)
PORT=8000
ENABLE_LANGFUSE=true
